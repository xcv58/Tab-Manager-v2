{
  "name": "tab-manager-v2",
  "version": "1.14.0",
  "description": "Quickly and conveniently manage your open tabs",
  "private": true,
  "scripts": {
    "analyze": "NODE_ENV=production node utils/analyze.js",
    "release": "standard-version",
    "build": "concurrently yarn:build:*",
    "_build": "node utils/build.js",
    "build:chrome": "yarn _build",
    "build:firefox": "TARGET_BROWSER=firefox yarn _build",
    "start": "concurrently yarn:start:*",
    "_start": "node utils/webserver.js",
    "start:chrome": "yarn _start",
    "start:firefox": "TARGET_BROWSER=firefox PORT=3001 yarn _start",
    "codecov": "codecov",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "jest": "jest --maxWorkers=1",
    "test-watch": "yarn jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "dependencies": {
