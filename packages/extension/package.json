    "terser-webpack-plugin": "5.3.6",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.6.1",
