{
  "name": "tab-manager-v2",
  "version": "1.21.3",
  "description": "Quickly and conveniently manage your open tabs",
  "private": true,
  "scripts": {
    "analyze": "NODE_ENV=production node utils/analyze.js",
    "release": "standard-version",
    "build": "run-p build:*",
    "_build": "NODE_ENV=production node utils/build.js",
    "build:chrome": "TARGET_BROWSER=chrome yarn _build",
    "build:firefox": "TARGET_BROWSER=firefox yarn _build",
    "start": "run-p start:*",
    "_start": "node utils/webserver.js",
    "start:chrome": "TARGET_BROWSER=chrome yarn _start",
    "start:firefox": "TARGET_BROWSER=firefox PORT=3001 yarn _start",
    "stat": "rm -f build/SHA256SUMS.txt; sha256sum build/build_*/* build/build_*.zip | tee build/SHA256SUMS.txt; cat build/build_*/* | sha256sum | tee -a build/SHA256SUMS.txt",
    "zip": "run-s zip:*",
    "_zip": "zip -X --compression-method deflate -9 -r --junk-paths",
    "ls": "ls -lh build/build*.zip",
    "zip:chrome": "yarn _zip build/build_chrome.zip build/build_chrome/*",
    "zip:firefox": "yarn _zip build/build_firefox.zip build/build_firefox/*",
    "codecov": "codecov",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test": "jest --maxWorkers=1",
    "test-watch": "yarn jest --watch"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.9.1",
    "@mui/styles": "5.9.1",
    "autoprefixer": "10.4.8",
    "autosuggest-highlight": "3.3.4",
    "classnames": "2.3.1",
    "fuzzy": "0.1.3",
    "lodash.debounce": "4.0.8",
    "lodash.merge": "4.6.2",
    "loglevel": "1.8.0",
    "match-sorter": "6.3.1",
    "mobx": "6.6.1",
    "mobx-react-lite": "3.4.0",
    "moment": "2.29.4",
    "mousetrap": "1.6.5",
    "postcss": "8.4.16",
    "postcss-loader": "7.0.1",
    "react": "18.2.0",
    "react-command-palette": "0.21.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-resize-detector": "7.1.2",
    "react-router-dom": "6.3.0",
    "react-window": "1.8.7",
    "rebound": "0.1.0",
    "tailwindcss": "3.1.8",
    "use-system-theme": "0.1.1",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@types/chrome": "0.0.199",
    "@types/classnames": "2.3.1",
    "@types/jest": "27.5.2",
    "@types/lodash.debounce": "4.0.7",
    "@types/match-sorter": "6.0.0",
    "@types/mousetrap": "1.6.9",
    "@types/react": "18.0.22",
    "@types/react-custom-scrollbars": "4.0.10",
    "@types/react-dom": "18.0.7",
    "@types/react-router-dom": "5.3.3",
    "@types/react-window": "1.8.5",
    "@types/webpack-env": "1.18.0",
    "babel-eslint": "10.1.0",
    "chrome-webstore-upload-cli": "2.1.0",
    "clean-webpack-plugin": "4.0.0",
    "codecov": "3.8.3",
    "copy-webpack-plugin": "11.0.0",
    "coveralls": "3.1.1",
    "css-loader": "6.7.1",
    "dotenv": "16.0.3",
    "eslint-plugin-typescript": "0.14.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "glob-all": "3.3.0",
    "hard-source-webpack-plugin": "0.13.1",
    "html-loader": "4.1.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "mini-css-extract-plugin": "2.6.1",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "postcss-loader": "7.0.1",
    "progress-bar-webpack-plugin": "2.1.0",
    "purgecss-webpack-plugin": "4.1.3",
    "react-axe": "3.5.4",
