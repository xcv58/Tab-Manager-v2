    "@testing-library/react": "16.3.0",
    "@types/chrome": "0.0.313",
    "@types/classnames": "2.3.4",
    "@types/jest": "29.5.14",
    "@types/lodash.debounce": "4.0.9",
    "@types/match-sorter": "6.0.0",
    "@types/mousetrap": "1.6.15",
    "@types/react": "18.3.20",
    "@types/react-custom-scrollbars": "4.0.13",
