{
  "name": "tab-manager-v2",
  "version": "1.8.2",
  "description": "Quickly and conveniently manage your open tabs",
  "scripts": {
    "analyze": "NODE_ENV=production node utils/analyze.js",
    "jest": "jest --maxWorkers=1",
    "build": "NODE_ENV=production node utils/build.js",
    "codecov": "codecov",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "deploy": "yarn build && yarn zip && ls -lh build.zip",
    "release": "standard-version",
    "upload:chrome": "node -r dotenv/config ./node_modules/.bin/webstore upload --source build.zip",
    "publish:chrome": "node -r dotenv/config ./node_modules/.bin/webstore publish",
    "publish:firefox": "web-ext sign --source-dir build --channel=listed || echo 'Publish to Mozilla Add-ons may failed because https://github.com/mozilla/web-ext/issues/804'",
    "publish-extension": "yarn publish:chrome && yarn publish:firefox",
    "zip": "zip -X --compression-method deflate -r --junk-paths build.zip build/*",
    "lint:ts": "standard --parser typescript-eslint-parser --plugin typescript ./**/*.tsx",
    "lint": "standard",
    "prettier": "prettier --no-semi --single-quote --write",
    "lintfix": "yarn prettier 'src/**/*.{js,ts,tsx}' && yarn lint --fix && yarn lint:ts --fix",
    "start": "node utils/webserver.js",
    "test": "yarn lint && yarn lint:ts && yarn jest",
    "test-watch": "yarn jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "dependencies": {
    "@material-ui/core": "4.9.10",
    "@material-ui/icons": "4.9.1",
    "classnames": "2.2.6",
    "fuzzy": "0.1.3",
    "lodash.debounce": "4.0.8",
    "lodash.merge": "4.6.2",
    "loglevel": "1.6.7",
    "mobx": "5.15.4",
    "mobx-react": "6.2.2",
    "mousetrap": "1.6.5",
    "react": "16.13.1",
    "react-command-palette": "0.12.0-0",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dom": "16.13.1",
    "react-resize-detector": "4.2.1",
    "rebound": "0.1.0",
    "tailwindcss": "1.2.0",
    "use-system-theme": "0.0.4",
    "webextension-polyfill": "0.6.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.104",
    "@types/jasmine": "3.5.10",
    "@types/jest": "25.2.1",
    "@types/react": "16.9.34",
    "@types/react-custom-scrollbars": "4.0.6",
    "@types/react-dom": "16.9.6",
    "@types/webpack-env": "1.15.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-eslint": "10.1.0",
    "chrome-webstore-upload-cli": "1.2.0",
    "clean-webpack-plugin": "3.0.0",
    "codecov": "3.6.5",
    "copy-webpack-plugin": "5.1.1",
    "coveralls": "3.0.11",
    "css-loader": "3.5.2",
    "dotenv": "8.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-plugin-typescript": "0.14.0",
    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "glob-all": "3.2.1",
    "hard-source-webpack-plugin": "0.13.1",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.5",
    "jest": "25.3.0",
    "lint-staged": "10.1.3",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.4",
    "progress-bar-webpack-plugin": "2.1.0",
    "purgecss-webpack-plugin": "2.1.2",
    "react-axe": "3.4.1",
    "react-test-renderer": "16.13.1",
    "regenerator-runtime": "0.13.5",
    "samsam": "1.3.0",
    "sinon": "9.0.2",
    "sinon-chrome": "3.0.1",
    "speed-measure-webpack-plugin": "1.3.3",
    "standard": "14.3.3",
    "standard-version": "7.1.0",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "ts-jest": "25.3.1",
    "ts-loader": "6.2.2",
    "typescript": "3.8.3",
    "typescript-eslint-parser": "22.0.0",
    "web-ext": "4.1.0",
    "webpack": "4.40.2",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-dev-server": "3.10.3",
    "write-file-webpack-plugin": "4.5.1"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "parser": "babel-eslint",
    "globals": [
      "screen",
      "HTMLElement",
      "HTMLInputElement",
      "page",
      "browser"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "standard --fix"
    ],
    "*.{ts,tsx}": [
      "yarn prettier",
      "standard --parser typescript-eslint-parser --plugin typescript --fix"
    ],
    "*.{json,css,md}": [
      "yarn prettier"
    ]
  }
}
