{
  "name": "tab-manager-v2",
  "version": "0.4.6",
  "description": "Quickly and conveniently manage your open tabs",
  "scripts": {
    "analyze": "NODE_ENV=production node utils/analyze.js",
    "ava": "nyc ava",
    "build": "node utils/build.js",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "deploy": "NODE_ENV=production yarn build && yarn zip",
    "zip": "zip -X --compression-method deflate -r build.zip build/*",
    "lint": "standard",
    "lintfix": "yarn lint -- --fix",
    "start": "node utils/webserver.js",
    "test": "yarn lint && yarn ava",
    "test-watch": "yarn ava --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "dependencies": {
    "chrome-extension-async": "^3.2.4",
    "dom-css": "^2.1.0",
    "fuzzy": "^0.1.3",
    "material-ui": "1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mousetrap": "^1.6.1",
    "react": "^16.2.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.2.0",
    "react-flip-move": "^2.10.0",
    "rebound": "^0.0.15"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.17",
    "@storybook/addon-links": "^3.2.17",
    "@storybook/react": "^3.2.17",
    "ava": "^0.24.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "2.0.0-beta.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.3.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.6",
    "fs-extra": "^5.0.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "nyc": "^11.4.1",
    "sinon": "^4.1.3",
    "sinon-chrome": "^2.2.1",
    "standard": "^11.0.0-beta.0",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "chrome",
      "screen"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
